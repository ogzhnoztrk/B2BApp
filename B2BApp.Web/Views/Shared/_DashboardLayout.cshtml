    <script type="text/javascript">


        $(document).ready(function () {
            // Sayfa yüklendiğinde localStorage'dan aktif link'i al
            var activeLink = localStorage.getItem('activeLink');
            // Tüm nav-link elementlerini seç
            $('.nav-link').click(function () {
                // Önce tüm nav-link elementlerinden active class'ını kaldır
                $('.nav-link').removeClass('active');
                // Tıklanan elemente active class'ını ekle
                $(this).addClass('active');

                // Tıklanan link'in text içeriğini localStorage'a kaydet
                var linkText = $(this).find('.item-name').text();
                localStorage.setItem('activeLink', linkText);
            });

            // Eğer daha önce seçilmiş bir link varsa, onu tekrar active yap
            if (activeLink) {
                $('.nav-link .item-name').each(function () {
                    if ($(this).text() === activeLink) {
                        $(this).closest('.nav-link').addClass('active');
                    }
                });
            }
        });

    </script>


